{
  "doctype": "DocType",
  "name": "Credit Note Taxes and Charges",
  "module": "credit_note",
  "document_type": "Setup",
  "istable": 1,
  "editable_grid": 1,
  "engine": "InnoDB",
  "field_order": [
    "charge_type",
    "row_id",
    "account_head",
    "col_break_1",
    "description",
    "included_in_print_rate",
    "included_in_paid_amount",
    "accounting_dimensions_section",
    "cost_center",
    "dimension_col_break",
    "section_break_8",
    "rate",
    "section_break_9",
    "account_currency",
    "tax_amount",
    "total",
    "tax_amount_after_discount_amount",
    "column_break_13",
    "base_tax_amount",
    "base_total",
    "base_tax_amount_after_discount_amount",
    "item_wise_tax_detail",
    "dont_recompute_tax",
    "category",
    "add_deduct_tax",
    "gst_tax_type"
  ],
  "fields": [
    {
      "fieldname": "charge_type",
      "fieldtype": "Select",
      "label": "Type",
      "options": "\nActual\nOn Net Total\nOn Previous Row Amount\nOn Previous Row Total\nOn Item Quantity",
      "reqd": 1,
      "in_list_view": 1,
      "columns": 2
    },
    {
      "fieldname": "row_id",
      "fieldtype": "Data",
      "label": "Reference Row #",
      "depends_on": "eval:[\"On Previous Row Amount\", \"On Previous Row Total\"].indexOf(doc.charge_type)!==-1"
    },
    {
      "fieldname": "account_head",
      "fieldtype": "Link",
      "label": "Account Head",
      "options": "Account",
      "reqd": 1,
      "allow_on_submit": 1,
      "in_list_view": 1,
      "columns": 2,
      "search_index": 1
    },
    {
      "fieldname": "col_break_1",
      "fieldtype": "Column Break",
      "width": "50%"
    },
    {
      "fieldname": "description",
      "fieldtype": "Small Text",
      "label": "Description",
      "reqd": 1,
      "print_width": "300px",
      "width": "300px"
    },
    {
      "fieldname": "included_in_print_rate",
      "fieldtype": "Check",
      "label": "Is this Tax included in Basic Rate?",
      "default": "0",
      "description": "If checked, the tax amount will be considered as already included in the Print Rate / Print Amount",
      "print_hide": 1,
      "print_width": "150px",
      "report_hide": 1,
      "width": "150px"
    },
    {
      "fieldname": "included_in_paid_amount",
      "fieldtype": "Check",
      "label": "Considered In Paid Amount",
      "default": "0",
      "depends_on": "eval:['Sales Taxes and Charges Template', 'Payment Entry'].includes(parent.doctype)",
      "description": "If checked, the tax amount will be considered as already included in the Paid Amount in Payment Entry"
    },
    {
      "fieldname": "accounting_dimensions_section",
      "fieldtype": "Section Break",
      "label": "Accounting Dimensions"
    },
    {
      "fieldname": "cost_center",
      "fieldtype": "Link",
      "label": "Cost Center",
      "options": "Cost Center",
      "allow_on_submit": 1,
      "default": ":Company"
    },
    {
      "fieldname": "dimension_col_break",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "section_break_8",
      "fieldtype": "Section Break"
    },
    {
      "fieldname": "rate",
      "fieldtype": "Float",
      "label": "Tax Rate",
      "in_list_view": 1,
      "columns": 2
    },
    {
      "fieldname": "section_break_9",
      "fieldtype": "Section Break"
    },
    {
      "fieldname": "account_currency",
      "fieldtype": "Link",
      "label": "Account Currency",
      "options": "Currency",
      "fetch_from": "account_head.account_currency",
      "read_only": 1
    },
    {
      "fieldname": "tax_amount",
      "fieldtype": "Currency",
      "label": "Amount",
      "options": "currency",
      "in_list_view": 1,
      "columns": 2
    },
    {
      "fieldname": "total",
      "fieldtype": "Currency",
      "label": "Total",
      "options": "currency",
      "in_list_view": 1,
      "columns": 2,
      "read_only": 1
    },
    {
      "fieldname": "tax_amount_after_discount_amount",
      "fieldtype": "Currency",
      "label": "Tax Amount After Discount Amount",
      "options": "currency",
      "read_only": 1
    },
    {
      "fieldname": "column_break_13",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "base_tax_amount",
      "fieldtype": "Currency",
      "label": "Amount (Company Currency)",
      "options": "Company:company:default_currency",
      "read_only": 1
    },
    {
      "fieldname": "base_total",
      "fieldtype": "Currency",
      "label": "Total (Company Currency)",
      "options": "Company:company:default_currency",
      "read_only": 1
    },
    {
      "fieldname": "base_tax_amount_after_discount_amount",
      "fieldtype": "Currency",
      "label": "Tax Amount After Discount Amount (Company Currency)",
      "options": "Company:company:default_currency",
      "depends_on": "eval:parent.discount_amount",
      "read_only": 1
    },
    {
      "fieldname": "item_wise_tax_detail",
      "fieldtype": "Code",
      "label": "Item Wise Tax Detail",
      "hidden": 1,
      "read_only": 1
    },
    {
      "fieldname": "dont_recompute_tax",
      "fieldtype": "Check",
      "label": "Dont Recompute tax",
      "hidden": 1,
      "default": "0",
      "print_hide": 1,
      "read_only": 1
    },

    {
      "fieldname": "category",
      "fieldtype": "Select",
      "label": "Category",
      "options": "\nTotal\nValuation\nValuation and Total",
      "default": "Total",
      "in_list_view": 0
    },
    {
      "fieldname": "add_deduct_tax",
      "fieldtype": "Select",
      "label": "Add or Deduct",
      "options": "\nAdd\nDeduct",
      "default": "Add"
    },
    {
      "fieldname": "gst_tax_type",
      "fieldtype": "Data",
      "label": "GST Tax Type"
    }
  ],
  "idx": 0,
  "index_web_pages_for_search": 1,
  "links": [],
  "owner": "Administrator",
  "permissions": [],
  "row_format": "Dynamic",
  "sort_field": "modified",
  "sort_order": "ASC",
  "states": []
}
